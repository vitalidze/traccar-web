<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.7.0//EN"
        "http://gwtproject.org/doctype/2.7.0/gwt-module.dtd">
<module>
    <inherits name='com.sencha.gxt.theme.neptune.Theme' />

    <replace-with class="com.sencha.gxt.theme.neptune.client.base.menu.Css3MenuItemAppearance">
        <when-type-is class="com.sencha.gxt.widget.core.client.menu.Item.ItemAppearance"/>
    </replace-with>

    <replace-with class="org.traccar.web.client.theme.WindowAppearance">
        <when-type-is class="com.sencha.gxt.widget.core.client.Window.WindowAppearance"/>
    </replace-with>

    <replace-with class="org.traccar.web.client.theme.ToolbarAppearance">
        <when-type-is class="com.sencha.gxt.widget.core.client.toolbar.ToolBar.ToolBarAppearance"/>
    </replace-with>
</module>